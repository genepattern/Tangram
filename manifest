p12_MODE=
p12_TYPE=TEXT
p12_default_value=cells
p12_description=Mode of alignment/mapping of either\:<br>\n&nbsp;&nbsp;1. "cells"; mapping at single-cell resolution<br>\n&nbsp;&nbsp;2. "clusters"; averages single cells of the same cluster via the cell type metadata field annotation<br>\n&nbsp;&nbsp;3. "constrained"; <b>for deconvolution</b>, adds a filter term to the loss function<br>\n\nDefault is "cells."
p12_fileFormat=
p12_flag=--alignment_mode 
p12_name=alignment.mode
p12_numValues=1..1
p12_optional=
p12_prefix=--alignment_mode 
p12_prefix_when_specified=--alignment_mode 
p12_type=java.lang.String
p12_userSuppliedValuesOK=False
p12_value=clusters\=clusters;cells\=cells;constrained\=constrained
